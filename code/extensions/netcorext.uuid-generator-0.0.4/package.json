{
	"name": "uuid-generator",
	"displayName": "UUID Generator",
	"description": "Generate a UUID",
	"version": "0.0.4",
	"license": "MIT",
	"publisher": "netcorext",
	"author": {
		"name": "Aren Chen",
		"email": "gucci313031@gmail.com"
	},
	"homepage": "https://github.com/arenchen/vscode-uuid-generator",
	"icon": "images/icon.png",
	"bugs": {
		"url": "https://github.com/arenchen/vscode-uuid-generator/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/arenchen/vscode-uuid-generator.git"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"keywords": [
		"GUID",
		"UUID"
	],
	"main": "./out/src/extension.js",
	"activationEvents": [
		"onCommand:uuid.generate",
		"onCommand:uuid.duplicate"
	],
	"contributes": {
		"commands": [
			{
				"command": "uuid.generate",
				"title": "Generate a UUID"
			},
			{
				"command": "uuid.duplicate",
				"title": "Duplicate a UUID"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorTextFocus",
					"command": "uuid.generate",
					"group": "uuid"
				},
				{
					"when": "editorHasMultipleSelections",
					"command": "uuid.duplicate",
					"group": "uuid"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"tsc-watch": "^1.1.36",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25"
	},
	"dependencies": {
		"@types/uuid": "^3.4.4",
		"uuid": "^3.3.2"
	},
	"__metadata": {
		"id": "802d93e3-0926-4d33-8408-b00d92d6cd0c",
		"publisherId": "98f9b0e7-cf44-4d87-b3e3-9319e99f492c",
		"publisherDisplayName": "netcorext"
	}
}
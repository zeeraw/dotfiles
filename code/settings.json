{
    "window.zoomLevel": 0,
    "editor.rulers": [
        80
    ],
    "go.autocompleteUnimportedPackages": true,
    "go.buildOnSave": "package",
    "go.lintOnSave": "package",
    "go.vetOnSave": "package",
    "go.buildTags": "",
    "go.buildFlags": [],
    "go.lintTool": "golint",
    "go.lintFlags": [],
    "go.vetFlags": [],
    "go.coverOnSave": false,
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.formatTool": "goimports",
    "go.formatFlags": [],
    "go.goroot": "/usr/local/opt/go/libexec",
    "go.gopath": "/usr/local/go",
    "go.toolsGopath": "/usr/local/gotools",
    "go.gocodeAutoBuild": false,
    "go.editorContextMenuCommands": {
        "toggleTestFile": false,
        "addTags": false,
        "removeTags": false,
        "testAtCursor": false,
        "testFile": false,
        "testPackage": false,
        "generateTestForFunction": false,
        "generateTestForFile": false,
        "generateTestForPackage": false,
        "addImport": true,
        "testCoverage": false,
        "playground": false
    },
    "[go]": {
        "editor.formatOnSave": true,
    },
    "files.associations": {
        "*.sql": "sql",
        "*.mysql": "sql",
        "*.postgresql": "postgres",
        ".env.prod": "dotenv",
        "*.postgres": "postgres",
        "hosts": "ansible-advanced",
        "*.yml": "yaml",
        "production": "ansible-advanced"
    },
    "editor.multiCursorModifier": "ctrlCmd",
    "[ansible-advanced]": {
        "editor.tabSize": 2
    },
    "yaml.format.enable": true,
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": true
    },
}